{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "removeComments": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "lib": ["es6"],
    "baseUrl": "./",
    "paths": {
      "src/*": ["./src/*"]
    }
  }
}
